<script>
KISSY.add('bb-main-right-bb-info', function (S, MC, Promise) {

  function X(module) {
    this.el = S.one(module);
    this.nick = null;
    this.config = {

    };
    this.init();
    this.getArticle();
    this.bind();
  }

  S.augment(X, {

    init : function () {
      var self = this;

      this.checkLogin().fin(function (v) {
        self.render(v);
      });

    },
    checkLogin : function () {
      //{{{
      var self = this, d = new Promise.Defer(), promise = d.promise, url = '//baobao.taobao.com/babyProp.htm', IO = S.require('ajax');

      new IO({
        url : url,
        dataType : 'jsonp',
        cache : false,
        scriptCharset : 'gbk',
        success : function (data) {
          if (data && data.step) {
            d.resolve(data);
          } else {
            d.reject(data);
          }
        },
        error : function () {
          d.reject({
            isLogin : false
          });
        }
      });

      return promise.then(function (data) {

        MC.fire('baby:profileFetched', {
          data : data
        });

        return data;
      }, function () {
        MC.fire('baby:profileFail');
      });//}}}
    },
    render : function (data) {
      var//{{{
        self = this, line1 = S.one('.J_line_one', self.el), line2 = S.one('.J_line_two', self.el);

      var bir;
      var nick = data.nick;
      var tip = '';

      if(!data || !(bir = data.bir)) {
        if(data.isLogin) {
          line1.html('<span class="colored J_edit_profile">��д��������</span>');
          line2.html('���������Ƽ�');
        } else {
          line1.html('<span class="colored J_login">�����Ա�ĸӤ</span>');
          line2.html('���������Ƽ�');
        }
      } else {
        var birthday = bir.slice(0, 4) + '-' + bir.slice(4, 6) + '-' + bir.slice(6, 8),
          pregnant = new Date().getTime() - new Date(birthday).getTime() < -1000 * 60 * 60 * 24 * 7 * 40 ? true : false;

        var age = data.ageDesc;
        if (pregnant) {
          //δ����
          age = '��������';
        }

        if(data.isPredict) {
          nick = '���ǲ²���ı���';
          tip = age + '<br/><span class="colored J_edit_profile">�¶���!</span>|<span class="colored J_edit_profile">�µĲ���!</span> ';

          self.baby = {
            babyBirthday: birthday,
            babyHeight: data.height,
            babySex: data.sex,
            babyWeight: data.weight
          };
        } else {
          tip = '<span class="colored J_edit_profile">' + age + '</span>';
        }

        line1.html(nick);
        line2.html(tip);
        self.renderAvatar(data, pregnant);


      }

      self.updateArticle(data);
      //}}}
    },
    bind : function () {
      //{{{
      var self = this;

      self.el.delegate('click', '.J_login', function () {
        S.use('kg/mini-login/6.3.8/', function (S, ML) {
          var miniLogin = new ML();
          miniLogin.show();
          miniLogin.on('login',function(){
            self.init();
          });
        });
      });

      S.use('kg/baby-profile/6.0.1/,kg/baby-profile/6.0.1/index.css', function (S, M) {

        var profile = new M.profile({
          "nobackground" : false
        });

        self.el.delegate('click', '.J_edit_profile', function (e) {
          profile.show(self.baby);
        });

        profile.on('createSuccess', function (e) {
          self.init();
        });

        profile.on('createError', function (e) {
          self.init();
        });
      });//}}}
    },
    getArticle : function () {
      //{{{
      var self = this;

      self.articles = S.JSON.parse(S.one('.J_articles').html());
      //}}}
    },
    renderAvatar : function (data, pregnant) {
      var avatar = S.one('.avatar', this.el);//{{{

      if (!data) return;

      if (data.gender === '��') {
        avatar.css('background-position', '0 -60px');
      } else if (data.gender === 'Ů') {
        avatar.css('background-position', '0 -120px');
      } else {
        avatar.css('background-position', '0 0');
      }

      if (pregnant) {
        avatar.css('background-position', '0 -60px');
      }//}}}
    },
    updateArticle : function (data) {
      //{{{
      var self = this, setArticle = function (article) {

          var titleTpl = '<a href={href} target="_blank">{title}</a>', contentTpl = '<a href={href} target="_blank">{content}</a>';

          S.one('.J_title', self.el).html(S.substitute(titleTpl, {
            title : article.title,
            href : article.href
          }));
          S.one('.J_content', self.el).html(S.substitute(contentTpl, {
            content : article.thumbnail.length > 32 ? article.thumbnail.slice(0, 31) + '...' : article.thumbnail,
            href : article.href
          }));

        };

      var _num = 0;
      if (data && data.ageDesc) {
        S.each(self.articles, function (v, k) {
          if (v.ageDesc === data.ageDesc) {
            _num = k;
          }
        });
      }

      setArticle(self.articles[_num]);

      //}}}
    }
  });

  return X;
}, {
  requires : ['market/msg', 'promise', 'ajax', 'cookie', 'tbc/mini-login/1.4.0/', 'node', 'base', 'json']
});

KISSY.add('bb-main-right-bb-qa', function (S, JSON) {

  function X(module) {
    this.el = S.one(module);

    this.config = {
      url : '//wenda.taobao.com/api/thread_list.htm',
      src : 'baobao',
      category : 1047,
      st : 1
    };

    this.init();
  }

  S.augment(X, {
    init : function () {

      var self = this;

      self.fetchData().done(function (data) {
        self.render(data);
      }, function () {
        var data = JSON.parse(S.one('.J_threadListData', self.el).val());
        self.render(data, true);
      });

    },
    fetchData : function () {
      //{{{
      var self = this, IO = S.require('ajax'), Promise = S.require('promise'), d = new Promise.Defer(), promise = d.promise;

      new IO({
        url : self.config.url,
        data : {
          src : self.config.src,
          category : self.config.category,
          st : self.config.st,
          pageSize : 5
        },
        cache : false,
        dataType : 'jsonp',
        scriptCharset : 'gbk',
        success : function (data) {
          if (data.threadList && data.threadList.length) {
            d.resolve(data);
            return;
          }
          d.reject();
        },
        error : function () {
          d.reject();
        }
      });

      return promise;//}}}
    },
    render : function (data, tms) {
      //{{{
      var result = [], self = this, tpl = S.one('.J_tpl', self.el).html(), XTemplate = S.require('xtemplate');

      if (data && data.threadList) {

        S.each(data.threadList, function (v) {
          if (!tms) {
            v.before = self.timeBefore(v.createTime);
            v.nickname = v.nickname.slice(0, 2) + '***';
            v.subject = v.subject.length > 15 ? v.subject.slice(0, 14) + '...' : v.subject;
            v.href = '//wenda.taobao.com/thread/detail/' + v.threadId + '.htm';
          }
          result.push(v);
        });

        S.one('.qa-list', self.el).html(new XTemplate(tpl).render({
          threadList : result
        }));
      }
      //}}}
    },
    timeBefore : function (time) {
      //{{{
      var now = new Date().getTime(), gap = now - time;

      if (gap > 0 && gap <= 60 * 1000) {
        return Math.floor(gap / 1000) + '��';
      } else if (gap > 60 * 1000 && gap <= 60 * 60 * 1000) {
        return Math.floor(gap / (1000 * 60)) + '��';
      } else if (gap > 60 * 60 * 1000 && gap <= 24 * 60 * 60 * 1000) {
        return Math.floor(gap / (1000 * 60 * 60)) + 'Сʱ';
      } else if (gap > 60 * 60 * 1000 * 24 && gap <= 60 * 60 * 1000 * 24 * 30) {
        return Math.floor(gap / (1000 * 60 * 60 * 24)) + '��';
      } else if (gap > 60 * 60 * 1000 * 24 * 30 && gap < 60 * 60 * 1000 * 24 * 30 * 12) {
        return Math.floor(gap / (1000 * 60 * 60 * 24 * 30)) + '��';
      } else {
        return Math.floor(gap / (1000 * 60 * 60 * 24 * 30 * 12)) + '��';
      }//}}}
    }
  });

  return X;

}, {
  requires : ['json', 'ajax', 'node', 'base', 'promise', 'xtemplate']
});

KISSY.add('bb-main-right', function (S, MC, Promise, XTemplate, JSON) {

  // �����嵥
  var shoppingList = (function () {
    var method = {
      init : function ($elem, options) {
        options.$content = S.one('.J_shoppingList', $elem);
        options.tmpl = new XTemplate(S.one('.J_shoppingListTmpl', $elem).html());
        options.data = method.getData($elem);

        method.bind($elem, options);

        // method.render(parseInt(Math.random()*(10)), options);
      },
      bind : function ($elem, options) {
        MC.promiseOn('baby:profileFetched', function (rs) {
          var step = rs.data.step;
          method.render(step, options);
        });

        MC.promiseOn('baby:profileFail', function (e) {
          method.render(parseInt(Math.random() * (10)), options);
        });
      },
      getData : function ($elem) {
        var str = S.one('.J_shoppingListData', $elem).val();
        return JSON.parse(str);
      },
      render : function (index, options) {
        options.$content.html(options.tmpl.render({
          list : options.data.results[index]
        }));
      }
    };

    return function (elem) {
      var $elem = S.one(elem);
      var options = {};

      method.init($elem, options);
    }

  })();

  // ģ���ʼ������
  function X(box, module) {

    // code here

    //��������
    S.use('bb-main-right-bb-info', function (S, BB) {
      new BB(module);
    });

    //ĸӤ�ʴ�
    S.use('bb-main-right-bb-qa', function (S, QA) {
      new QA(module);
    });

    shoppingList(module);
  }

  return X;

}, {
  requires : ['market/msg', 'promise', 'xtemplate', 'json']
});

</script>
<script>KISSY.use("node,gallery/datalazyload/1.0.1/index",function(c,a,b){window.renderCallback=function(d,a){var e=c.one(d),b=e.attr("data-name");e.one(".tb-module").hasClass("tb-finish")||c.use(b,function(c,b){new b(d,a)})};a=c.DOM.viewportHeight();a=500<a?a:500;var f=new b({diff:{top:a/2,bottom:a/2},autoDestroy:false,placeholder:"//g.alicdn.com/s.gif"});c.each(c.all(".J_Module"),function(a){var b=c.one(a).one(".tb-module");b.hasClass("tb-lazyload")?f.addCallback(a,function(){renderCallback(a,b[0])}):renderCallback(a,b[0])})});</script></body></html>