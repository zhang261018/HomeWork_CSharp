    KISSY.use('node,dom,event,sizzle',function(S, Node, Dom, Event, Sizzle) {
        var  $=S.all;
       KISSY.use("market/ctrl,template,sizzle",function(S,Ctrl,Template) {
            S.all(".J_dynamic").each(function(v){
                var conf = v.one(".J_dynamic_data",v).val(),
                    tmpl = new Template($(".J_p4pItemTmp").html()).render;
                Ctrl.tms.dynamic(conf, v, tmpl,function(data) {
                  
                  for(var n = 0; n < data.length; n++){
                    var obj=KISSY.JSON.parse((data[n].ITEM_LIST[0].EXT).replace(/&quot;/g,'"'));
                    
                    var z=0;
                    S.each(obj,function(e){
                      
                     	z++;
                      if(z == 1){
                      	$(".purDisc",$(".purPro")).item(n).html(e);
                      
                      }
                      
                    })
                  }
                  

                });
            });
        });

        var dateTime = new Date($('.dateTime').val()).getTime()/1000;
        var sysTime = 1620739134;
        var time = dateTime - sysTime;
        function timeFormat(time){
            var D = double(Math.floor(time/(60*60*24)));
            var H = double(Math.floor(time/(60*60)%24));
            var M = double(Math.floor(time/60%60));
            var S = double(Math.floor(time%60));
            return D + '��'+ H + 'Сʱ'+ M + '��' + S + '��'
        }
        function double(num){
            return  num<10?'0'+num:num;
        }
        function countdown(time){
            var tim = setInterval(function(){
                $('.countDown span').html(timeFormat(time));
                --time
            },1000);
            if(time <=0){
                clearInterval(tim)
            }
        }
        countdown(time);
    })
</script>
<script>
    KISSY.use('jbc/jupicker2, jbc/lazyload, core', function (S, Jupicker, Lazy) {
        var $ = S.all;
        $(".J_Item").each(function (box) {
            var ids = box.attr("data-item");
            if(ids.length){
                Jupicker.getItems({
                    type: box.attr("data-type"),
                    juIds: ids
                }, function (data, html) {
                    box.html('<ul class="clearfix">' + html + '</ul>');
                    new Lazy({container: box});
                });
            }
        });

        $(".J_Brand").each(function (box) {
            var ids = box.attr("data-item");
            if(ids.length){
                Jupicker.getBrands({
                    type: box.attr("data-type"),
                    brandIds: ids
                }, function (data, html) {
                    box.html('<ul class="clearfix">' + html + '</ul>');
                    new Lazy({container: box});
                });
            }
        });
    });
</script>
<script>
    KISSY.use("node,dom", function(S, NODE,DOM) {
        var  $ = S.all,
                D = S.DOM,
                oDiv = $('#fx-sort'),
                aInput = oDiv.all(".sort"),
                arr = [];
        for (var i = 0; i < aInput.length; i++) {
            ovalue = aInput.item(i).val();
            if (ovalue == null) {
                ovalue = 0;
            }
            arr.push({
                "parentnode": aInput.item(i).parent(),
                "order": ovalue
            });
        }
        arr.sort(function($1, $2) {
            return $1.order - $2.order;
        });

        for (var i = 0; i < arr.length; i++) {
            oDiv.append(arr[i].parentnode);
        }
        // ¥��������
    })
</script>


