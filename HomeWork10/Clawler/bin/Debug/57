<script type="text/javascript" src="http://assets.taobaocdn.com/tbra/1.0/tbra-widgets.js"></script>
<script type='text/javascript'>
						(function(){
						//ͼƬ�����б� mengjia 070927
					    var Speed_1 = 10; //�ٶ�(����)
					    var Space_1 = 20; //ÿ���ƶ�(px)
					    var PageWidth_1 = 116 * 3; //��ҳ���
					    var interval_1 = 7000; //��ҳ���
					    var fill_1 = 0; //������λ
					    var MoveLock_1 = false;
					    var MoveTimeObj_1;
					    var MoveWay_1 = "right";
					    var Comp_1 = 0;
					    var AutoPlayObj_1 = null;
					    function GetObj(objName)
					    {
					        if (document.getElementById) 
					        {
					            return eval('document.getElementById("' + objName + '")');
					        }
					        else 
					        {
					            return eval('document.all.' + objName);
					        }
					    }
					    
					    function AutoPlay_1()
					    {
					        clearInterval(AutoPlayObj_1);
					        AutoPlayObj_1 = setInterval([ISL_GoDown_1,ISL_StopDown_1], interval_1);
					    }
					    
					    function ISL_GoUp_1()
					    {
					        if (MoveLock_1) return;
					        clearInterval(AutoPlayObj_1);
					        MoveLock_1 = true;
					        MoveWay_1 = "left";
							MoveTimeObj_1 = setInterval(ISL_ScrUp_1,Speed_1);
					    }
					    
					    function ISL_StopUp_1()
					    {
					        if (MoveWay_1 == "right") 
					        {
					            return;
					        };
					        clearInterval(MoveTimeObj_1);
					        if ((GetObj('scollCont').scrollLeft - fill_1) % PageWidth_1 != 0) 
					        {
					            Comp_1 = fill_1 - (GetObj('scollCont').scrollLeft % PageWidth_1);
					            CompScr_1()
					        }
					        else 
					        {
					            MoveLock_1 = false;
					        }
					        AutoPlay_1()
					    }
					    
					    function ISL_ScrUp_1()
					    {
					        if (GetObj('scollCont').scrollLeft <= 0) 
					        {
					            GetObj('scollCont').scrollLeft = GetObj('scollCont').scrollLeft + GetObj('list1_1').offsetWidth;
					        }
					        GetObj('scollCont').scrollLeft -= Space_1;
					    }
					    
					    function ISL_GoDown_1()
					    {
					        clearInterval(MoveTimeObj_1);
					        if (MoveLock_1) return;
					        clearInterval(AutoPlayObj_1);
					        MoveLock_1 = true;
					        MoveWay_1 = "right";
					        ISL_ScrDown_1();
					        MoveTimeObj_1 = setInterval(ISL_ScrDown_1, Speed_1);
					    }
					    
					    function ISL_StopDown_1()
					    {
					        if (MoveWay_1 == "left") 
					        {
					            return;
					        };
					        clearInterval(MoveTimeObj_1);
					        if (GetObj('scollCont').scrollLeft % PageWidth_1 - (fill_1 >= 0 ? fill_1 : fill_1 + 1) != 0) 
					        {
					            Comp_1 = PageWidth_1 - GetObj('scollCont').scrollLeft % PageWidth_1 + fill_1;
					            CompScr_1();
					        }
					        else 
					        {
					            MoveLock_1 = false;
					        }
					        AutoPlay_1();
					    }
					    
					    function ISL_ScrDown_1()
					    {
					        if (GetObj('scollCont').scrollLeft >= GetObj('list1_1').scrollWidth) 
					        {
					            GetObj('scollCont').scrollLeft = GetObj('scollCont').scrollLeft - GetObj('list1_1').scrollWidth;
					        }
					        GetObj('scollCont').scrollLeft += Space_1
					    }
					    
					    function CompScr_1()
					    {
					        if (Comp_1 == 0) 
					        {
					            MoveLock_1 = false;
					            return;
					        }
					        var num, TempSpeed = Speed_1, TempSpace = Space_1;
					        if (Math.abs(Comp_1) < PageWidth_1 / 2) 
					        {
					            TempSpace = Math.round(Math.abs(Comp_1 / Space_1));
					            if (TempSpace < 1) 
					            {
					                TempSpace = 1;
					            }
					        }
					        if (Comp_1 < 0) 
					        {
					            if (Comp_1 < -TempSpace) 
					            {
					                Comp_1 += TempSpace;
					                num = TempSpace;
					            }
					            else 
					            {
					                num = -Comp_1;
					                Comp_1 = 0;
					            }
					            GetObj('scollCont').scrollLeft -= num;
					            setTimeout(CompScr_1, TempSpeed);
					        }
					        else 
					        {
					            if (Comp_1 > TempSpace) 
					            {
					                Comp_1 -= TempSpace;
					                num = TempSpace;
					            }
					            else 
					            {
					                num = Comp_1;
					                Comp_1 = 0;
					            }
					            GetObj('scollCont').scrollLeft += num;
					            setTimeout(CompScr_1, TempSpeed);
					        }
					    }
					    
						//����һ��list2_1����list1_1�е�����
					    function picrun_ini()
					    {
					        GetObj("list2_1").innerHTML = GetObj("list1_1").innerHTML;
					        GetObj('scollCont').scrollLeft = fill_1 >= 0 ? fill_1 : GetObj('list1_1').scrollWidth - Math.abs(fill_1);
					        GetObj("scollCont").onmouseover = function()
					        {
					            clearInterval(AutoPlayObj_1);
					        }
					        GetObj("scollCont").onmouseout = function()
					        {
					            AutoPlay_1();
					        }
					        AutoPlay_1();
					    }
						YAHOO.util.Event.onDOMReady(picrun_ini);
						YAHOO.util.Event.on('leftBtn','mousedown',ISL_GoUp_1);
						YAHOO.util.Event.on('leftBtn','mouseup',ISL_StopUp_1);
						YAHOO.util.Event.on('leftBtn','mouseout',ISL_StopUp_1);
						
						YAHOO.util.Event.on('rightBtn','mousedown',ISL_GoDown_1);
						YAHOO.util.Event.on('rightBtn','mouseup',ISL_StopDown_1);
						YAHOO.util.Event.on('rightBtn','mouseout',ISL_StopDown_1);
						
						
						TB.widget.Tab = new function() {
						var Y = YAHOO.util, Dom = Y.Dom, Event = Y.Event, Lang = Y.Lang;
						var defConfig = {
							eventType: 'mouse',
							currentClass: 'selected',  /* li ��ǰѡ��״̬ʱ��className */
							tabClass: '',  /* ��Ϊ tab ��element�� className */
							tabPanelClass: '',  /* ��Ϊ tab panel ��element�� className */
							autoSwitchToFirst: true,  /* �Ƿ�Ĭ��ѡ�е�һ��tab */
							stopEvent: false,  /* ֹͣ�¼����� */
							delay: 0.1  /* available when eventType=mouse */
						};
					
						this.decorate = function(container, config) {
							container = Dom.get(container);
							config = Lang.merge(defConfig, config||{});
							/* ���ظ������ߵĿ�������ֻ�����Ե����߿ɼ��ķ���/���� */		
							var handle = {}, tab, tabPanels, tabTriggerBoxs, tabTriggers, delayTimeId;
							var parentNode = Dom.getAncestorByTagName(container,'div');
							tab = Dom.getFirstChild(container);  //container�µĵ�һ��el(ͨ��Ϊ<ul> or <ol>)����Ϊ��ǩ�б�
							tabTriggerBoxs  = tab.getElementsByTagName('li');
							
							if (config.tabClass) { //�������ָ����Ҫ������tab��ǩ��class�������className ��ȡԪ��
								tabTriggers = Dom.getElementsByClassName(config.tabClass, '*', container);
							} else {  //�����ȡ tab �µ���ν<a>��Ϊtab��ǩ
								tabTriggers = Array.copy(tab.getElementsByTagName('a')); /* Ĭ��ȡtab�µ�<a> */
							}
							if (config.tabPanelClass) { //�������ָ����Ҫ����Ϊtab����class,�����className ��ȡԪ��
								tabPanels = Dom.getElementsByClassName(config.tabPanelClass, '*', parentNode);
							} else { //����ֱ��ȡ����һ��Ԫ�����ʣ��Ԫ����Ϊ���
								tabPanels = Dom.getChildren(container).slice(1);
							}
							var onSwitchEvent = new Y.CustomEvent("onSwitch", null, '', Y.CustomEvent.FLAT);
							if (config.onSwitch) {
								onSwitchEvent.subscribe(config.onSwitch);
							}
							
							var focusHandler = function(ev) {
								if (delayTimeId)
									cacelHandler();
								var idx = tabTriggers.indexOf(this);
								handle.switchTab(idx);
								if (config.stopEvent) {
									try {
										Event.preventDefault(ev);
									}catch (e) {
										/* ignore */
									}
								}
								return !config.stopEvent;
							}
							var delayHandler = function(ev) {
								var target = this;
								delayTimeId = setTimeout(function(){
									focusHandler.call(target, ev);
								}, config.delay*1000);
								if (config.stopEvent)
									Event.preventDefault(ev);
								return !config.stopEvent;
							}
							var cacelHandler = function() {
								clearTimeout(delayTimeId);
							}
							if (config.eventType == 'mouse') {
								Event.on(tabTriggers, 'focus', focusHandler);
								Event.on(tabTriggers, 'mouseover', config.delay?delayHandler:focusHandler);
								Event.on(tabTriggers, 'mouseout', cacelHandler);
							}
							//else { // by yubo 2009-03-20  click �¼���ʼ��Ҫע��ģ�����stopEvent����Ч
							Event.on(tabTriggers, 'click', focusHandler);
							//}
					
							/* ���幫���ķ��� */
							Lang.augmentObject(handle, {
								switchTab: function(idx) {
									Dom.setStyle(tabPanels, 'display', 'none');
									Dom.removeClass(tabTriggerBoxs, config.currentClass);
									Dom.addClass(tabTriggerBoxs[idx], config.currentClass);
									Dom.setStyle(tabPanels[idx], 'display', 'block');
									onSwitchEvent.fire(idx);
								},
								subscribeOnSwitch: function(func) {
									onSwitchEvent.subscribe(func);
								}
							}, true);
							handle.onSwitch = handle.subscribeOnSwitch;
							
							/*��ʼ������*/
							Dom.setStyle(tabPanels, 'display', 'none');
							if (config.autoSwitchToFirst)
								handle.switchTab(0);
							
							/* ���ز������� */
							return handle;
						}
						};
						
						//�O���^����ؐ�ĵĂ���
						var setNum = function(){
							var ziti = YAHOO.util.Dom.getElementsByClassName('ziti','*',YAHOO.util.Dom.get('older'));
							var y = ziti.length;
							Array.forEach(ziti,function(item,i){
								var i = y-i;
								item.innerHTML = '<strong>' + i + '</strong>';
							})
						}
						YAHOO.util.Event.onDOMReady(setNum);
						TB.widget.SimpleSlide.decorate('slide', {currentClass:'selected',effect:'fade'});
						TB.widget.Tab.decorate('col', {tabPanelClass:'slide-panel',currentClass: 'selected'});
						(function(){
							var ul = YAHOO.util.Dom.get('panel');
							var lis = ul.getElementsByTagName('li');
							var slideLis = YAHOO.util.Dom.get('slide-col').getElementsByTagName('li');
							var timer = setInterval(function()
					        {
					            Array.forEach(lis, function(item, i)
					            {
									YAHOO.util.Dom.removeClass(slideLis[i],'selected');
					                if (YAHOO.util.Dom.getStyle(item, 'display') == 'block') 
					                {
					                    YAHOO.util.Dom.addClass(slideLis[i], 'selected');
					                }
					            })
					        }, 100)
						})()
					})()
					</script>
