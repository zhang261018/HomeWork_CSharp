<script>
  (function(){
      var S = KISSY,
               pre = ',tbc/search-suggest/'+'1.3.9/';
        S.config({
                packages: [
                    {
                        name: "tbc",
                        path: "//g.alicdn.com/tbc/",
                        ignorePackageNameInUri: true,
                        debug: false
                    }
                ]}
        );
      
    var mods = ['dom','index','plugin/history','new_suggest.css'],
        sort = ['history','list'];
      S.use(mods.join(pre),function(S,DOM,SearchSuggest,History){
          var sug = new SearchSuggest({
              plugins:[
                  new History()
              ],
              "sugConfig": {
                  sourceUrl: "//suggest.taobao.com/sug?k=1&area=ssrch",
                  node: "#q",
                  focused: true,
                  "resultFormat":""
              },
              "mods":{
                  "sort": sort
              }
           });
        });
        
                 
  })()
</script>


  
