        <script>
            KISSY.use('anim', function (S, Anim) {
                var D = S.DOM, E = S.Event;
                var tips = D.get('#J_TipsM');
                E.on(D.get('#J_Tips'), 'mouseover mouseout', function (e) {
                    var type = e.type;
                    var top, op;
                    if (type === 'mouseover') {
                        top = '-55px';
                        op = 1;
                    } else if (type === 'mouseout') {
                        top = '-50px';
                        op = 0;
                    }
                    Anim.stop(tips, false, false);
                    new Anim(tips, {
                        top : top,
                        opacity : op
                    }, 0.3).run();
                });
                //���õ�ǰ
                var setNavs = D.query('#mallTextNav li');
                var catId;
                for (var i = 0, len = setNavs.length; i < len; i++) {
                    catId = D.attr(setNavs[i], 'data-catid');
                    if (setCur === catId) {
                        D.addClass(D.get('a', setNavs[i]), 'cur');
                        break;
                    }
                }
                //���
                var J_Active = D.get('#J_Active');
                if (J_Active) {
                    var _aTop = D.offset(D.get('#content')).top;
                    var _tscroll = 0;
                    E.on(window, 'scroll', function (e) {
                        _tscroll = D.scrollTop(this);
                        //���
                        if (_tscroll > _aTop -310) {
                            if (S.UA.ie && S.UA.ie < 7) {
                                D.css(J_Active, {'margin-top': (_tscroll - _aTop) + 'px'});
                            } else {
                                D.css(J_Active, {position: 'fixed',top: 0});
                            }
                        } else {
                            D.css(J_Active, {position: 'absolute', top: '80px'});
                        }
                    });
                }
            });
        </script>
