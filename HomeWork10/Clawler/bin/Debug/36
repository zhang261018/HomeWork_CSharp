    <script type="text/javascript">
        (function( S ){
            var D = S.DOM, E = S.Event;
            S.add( "page", function( S ){
                var ELM_NAV = ".J_ServiceNav",
                    ELM_BOX = ".J_ServiceBox";
                var Page = function(){
                };
                S.augment( Page,{
                    init: function(){
                        this.bind();
                    },

                    bind: function(){
                        var ELM_O_NAV = D.get( ELM_NAV ),
                            ELM_LI_NAV = D.query( 'li', ELM_O_NAV ),
                            ELM_O_BOX = D.query( ELM_BOX );
                        E.on( ELM_LI_NAV,'click', function( e ){
                            e.halt();
                            S.each( ELM_LI_NAV, function( n, i ){
                                D.removeClass(n, 'cur' );
                                D.addClass( ELM_O_BOX[i], 'hidden');
                                if( n == e.target ){
                                    D.addClass(n, 'cur' );
                                    D.removeClass( ELM_O_BOX[i], 'hidden' );
                                }
                            })
                        });
                    }
                });
                return Page;
            });

            S.ready(function(){
                S.use("page",function( S, Page ){
                    var oPage = new Page();
                    oPage.init();
                    //��λTab
                    function tagParamhandle(){
                        var num = (location.href.indexOf('tabparam=') > -1) ?  location.href.split('tabparam=')[1].split('&')[0] : null;
                        if(num != null && S.inArray(num, ['0','1','2'])){
                            E.fire(S.all('li', S.get('.J_ServiceNav'))[num], 'click');
                        }
                    }

                    tagParamhandle();
                })
            });
        })( KISSY )
    </script>
    </div>
