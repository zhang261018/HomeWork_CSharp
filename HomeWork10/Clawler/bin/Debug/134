<script>
KISSY.use('switchable', function(S, Switchable){
	var Slide,Tabs;
if(KISSY.version == "1.1.6"){
  
   Slide = S.Slide,
    Tabs = S.Tabs;
}else{
	Slide = Switchable.Slide,
   Tabs = Switchable.Tabs;
}
    KISSY.ready( function(S) {
		var lastTab = document.getElementById("cod_tab_text_last");
		var DOM = S.DOM;
		var E = S.Event;
		lastTab.style.backgroundImage = "none";
        var slide = new Slide('.J_cod_slider', {
			delay : .2,
			effect : 'fade',
			easing : 'easeBoth',
			duration : .8,
			autoplay : true
		});
		var tabs = new Tabs('#J_cod_tabs', {
			switchTo: 0,
			autoplay: true,
			delay:  .1
		});

		tabs.on('switch', function(ev) {
			var secondTab = document.getElementById("cod_tab_text_second");
			var firstTab = document.getElementById("cod_tab_text_first");
			
	     if ( ev.currentIndex === 0) {
			 firstTab.style.backgroundImage = "none";
			 secondTab.style.backgroundImage = "url(//img.alicdn.com/tps/i2/T1sLq4Xf4wXXXXXXXX-2-29.jpg)";
			 secondTab.style.backgroundPosition = "right";
			 secondTab.style.backgroundRepeat = "no-repeat";
	     }
			
		 if (ev.currentIndex === 1) {
			firstTab.style.backgroundImage = "none";
			secondTab.style.backgroundImage = "none";
		 }

		 else if (ev.currentIndex === 2) {
			 secondTab.style.backgroundImage = "none";
			 firstTab.style.backgroundImage = "url(//img.alicdn.com/tps/i2/T1sLq4Xf4wXXXXXXXX-2-29.jpg)";
			 firstTab.style.backgroundPosition = "right";
			 firstTab.style.backgroundRepeat = "no-repeat";
		 }

		
		});

	});
});

</script>

