<script>
var part_video_required_conf_7 = [
    'http://sjs2.sinajs.cn/video/sinaplayer/js/page/player_v1.js',
    'http://n.sinaimg.cn/finance/forum/davos/videolist.js', 
    //'http://i0.sinaimg.cn/dy/js/jquery/jquery-1.7.2.min.js'
];
if(typeof bShare == "undefined") {
    var uuid = window.bShareOpt.uuid;
    part_video_required_conf_7.push('http://static.bshare.cn/b/buttonLite.js#style=-1&uuid=' + uuid + '&pophcol=2&lang=zh');
    part_video_required_conf_7.push('http://static.bshare.cn/b/bshareC0.js');
}
var playerConfig =      {
    url: "",  // flash播放器地址,
    container : "flashPlayer_conf_7",
    id: "myMovie",
    width: 720,
    height: 520,
    params : {
     allowNetworking : "all",
     allowScriptAccess : "always",
     wmode : "opaque",
     allowFullScreen : "true",
     quality : "high"
    },
    attributes: {},
    flashvars: {
        autoPlay: 0,  //是否自动播放
        loop: 0,        
        autoLoad: 1,
        thumbUrl: '',
        tj: 1,
        as: 0    },
    h5attr: {
        autoPlay: false,  //是否自动播放
        controls: true, //是否显示控制条
        loop: false,        
        poster: 'http://i0.sinaimg.cn/dy/slidenews/51_img/2016_10/39335_4882113_816447.jpg', //视频加载前欲加载的图片地址，即播放器一开始显示的截图
        preload: 'auto'
    }
}; 
function reSizePlayer(width, height) {
    if(typeof width != 'undefined' || typeof height != 'undefined') {
        var swf = $('#flashPlayer_conf_7').children()[0];
        swf.width = width;
        swf.height = height;
		$('#flashPlayer_conf_7').width(width).height(height);
     	$('#player_conf_7').width(width).height(height);
		$('#video_wrap_conf_7').height(height);
        return;
    }
    var swf, h ,w;
    if(typeof swf == 'undefined') {
        swf = document.getElementById('flashPlayer_conf_7').children[0];
        w = parseInt(swf.width);
        h = parseInt(swf.height);
    } else {
        throw new Error("playef not found error!"); 
    }
    var windowWidth = document.body.scrollWidth ;
    if(windowWidth >= 1360 && windowWidth < 1600) {
        swf.width = 840;
        swf.height = 520;
    } else if(windowWidth >= 1600) {
        swf.width = 1040;
        swf.height = 520;
    } else if(windowWidth < 1360) {
        swf.width = 640;    
        swf.height = 520;
    }
}
$widgetadd({
    id:'video_wrap_conf_7',
    loadTime: 'DOMContentLoaded',
    require: part_video_required_conf_7,
    isDepend: true,
    exports: function() {
        for(var i = 0; i < bShareEntry_conf_7.length; i++) {
            bShare.addEntry(bShareEntry_conf_7[i]);
        }
        window.videoList_conf_7 = new videoList('sider_conf_7', playerConfig, videos);
        window.videoList_conf_7.init();
        if(window._SINANEWSDATA.subjectName && window._SINANEWSDATA.subjectName == 'shadow_simple_subject') { // 新版两栏式才这样 
            reSizePlayer($('#player_conf_7').width(), $('#player_conf_7').height());
            window.onresize = function(){
                reSizePlayer($('#player_conf_7').width(), $('#player_conf_7').height());
            }
        } else {
            reSizePlayer($('#player_conf_7').width(), $('#player_conf_7').height());
        }
            reSizePlayer($('#player_conf_7').width(), $('#player_conf_7').height());
    }
});
</script>
