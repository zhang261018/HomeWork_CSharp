    function setPosition() {
        var w = $(window).width();
        if (w >= 1440) {
            var pageWidth = 1186;
            $(".main_content").css("width", "1186px");
        } else if (w < 1440) {
            var pageWidth = 1000;
            $(".main_content").css("width", "1000px");
        }
        $('#CCCFloatBarRight').css('margin-left', pageWidth / 2 + 10);
    };
