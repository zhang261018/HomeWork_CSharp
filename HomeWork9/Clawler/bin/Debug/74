<script>
$(function(){

function countWords(str) {
    var matches = str.match(/[\u00ff-\uffff]|\S+/g);
    return matches ? matches.length : 0;
}
$.fn.ignore = function(sel){
  return this.clone().find(sel||">*").remove().end();
};
var words = countWords($('table.d-block').ignore('.highlight').text());
if(words){
var time = Math.round(words/200);
$('<p class="post-reading-info"><i>字数：'+words+'，预计阅读时间：'+time+'min<i></p>').insertBefore('#cnblogs_post_body')

}

function setTheme(isDark){

if(isDark){
$('body').addClass('dark-theme');
localStorage.setItem('wayou.cnblog.theme.dark',isDark)
}else{
$('body').removeClass('dark-theme');
localStorage.removeItem('wayou.cnblog.theme.dark',isDark)
}

}

$('#themeSwitcher').on('change',function(){
setTheme($(this).prop('checked'));
})
var isDark  = localStorage.getItem('wayou.cnblog.theme.dark');
if(isDark){
$('#themeSwitcher').prop('checked','true');
$('#themeSwitcher').trigger('change');
}
})
</script>
