<h5>发送数据</h5>
<div class="cnblogs_code">
<pre> let btn = document.getElementById("btn"<span style="color: rgba(0, 0, 0, 1)">);
        btn.onclick </span>= <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> () {
            
            </span><span style="color: rgba(0, 0, 255, 1)">var</span> formdata=<span style="color: rgba(0, 0, 255, 1)">new</span> FormData(document.getElementById("userForm"<span style="color: rgba(0, 0, 0, 1)">));
            </span><span style="color: rgba(0, 0, 255, 1)">var</span> xhr = <span style="color: rgba(0, 0, 255, 1)">new</span><span style="color: rgba(0, 0, 0, 1)"> XMLHttpRequest();
            xhr.open(</span>"post", "file.php"<span style="color: rgba(0, 0, 0, 1)">);
        
            </span><span style="color: rgba(0, 128, 0, 1)">//</span><span style="color: rgba(0, 128, 0, 1)"> 监听上传进度</span>
            xhr.upload.onprogress=<span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)">(ev){
                let percent</span>=(ev.loaded/ev.total)*100+'%';
<span style="color: rgba(0, 0, 0, 1)">                console.log(percent)
            }
            xhr.send(formdata);
        
            xhr.onload </span>= <span style="color: rgba(0, 0, 255, 1)">function</span><span style="color: rgba(0, 0, 0, 1)"> () {
                </span><span style="color: rgba(0, 0, 255, 1)">if</span> (xhr.status == 200<span style="color: rgba(0, 0, 0, 1)">) {
                    </span><span style="color: rgba(0, 128, 0, 1)">//</span><span style="color: rgba(0, 128, 0, 1)">...</span>
<span style="color: rgba(0, 0, 0, 1)">                }
            }

        }</span></pre>
</div>
