        var img_list=[];
        var img_node=$(".img-wrap img");
        img_node.each(function () {
          img_list.push({ 
              src:$(this).attr("data-src"),
              show:false
            });
            
        });
        var page_hash=window.location.hash;
        var page_number=0;
        if(page_hash)
        {
            var pattern=/p=\d*/;
            var number=page_hash.match(pattern)[0].split("p=")[1];
            if(number>=2)
            {
                //�����Ƿ����
                page_number=(number-2); // p=2��Ӧ�����±�Ϊ0

            }else if(number>(img_list.length+1))
            {
                page_number=img_list.length-1;
            }
        

        }
        order_img(page_number,img_list[page_number].src,true);
        function order_img(index,src,left){
           
            if(index<0||index>=img_list.length||img_list[index].show)
            {}
            else {
            $(img_node[index]).attr("src",src);
            img_list[index].show=true;
            //��ʱ����
            var timer=setTimeout(function () {
                if(left&&index>0)
                {
                    order_img(index-1,img_list[index-1].src,true);
                   
                }
                if(index<img_list.length-1)
                {
                    order_img(index+1,img_list[index+1].src,false);
                }
            },5000);
            $(img_node[index]).on("load",function () {
                clearTimeout(timer);
                //������Ҫ���ÿ�� ���body ��/��<1.5 ͼƬΪbody�Ŀ�
                var body_height=$("body").height();
                var body_width=$("body").width();
                if(body_width/body_height<1.5)
                {
                    $(img_node[index]).width(body_width);
                }
                else {

                    $(img_node[index]).height(body_height);
                }
                if(left&&index>0)
                {
                    order_img(index-1,img_list[index-1].src,true);
                   
                }
                if(index<img_list.length-1)
                {
                    order_img(index+1,img_list[index+1].src,false);
                }
                       
            });
            $(img_node[index]).on("error",function () {
                if(left&&index>0)
                {
                    order_img(index-1,img_list[index-1].src,true);
                   
                }
                if(index<img_list.length-1)
                {
                    order_img(index+1,img_list[index+1].src,false);
                }
            });
            }


        }
        //����ͼͼƬ���� end
        });

