var Url_domainname=location.href;
var Url_domainname1 = '';
var $=jQuery;
//登录框
$("#ccc").click(function(){
	$("#ccc").css({"background":"#e8e8e8","color":"#1E63B0"});
	$("#SI_Top_LoginLayer").show();
});
