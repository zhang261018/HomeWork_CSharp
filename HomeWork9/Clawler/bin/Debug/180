<span style="color: rgba(0, 128, 128, 1)"> 5</span> mysql&gt; <span style="color: rgba(0, 0, 255, 1)">select</span> *<span style="color: rgba(0, 0, 0, 1)"> from t_log;
</span><span style="color: rgba(0, 128, 128, 1)"> 6</span> +----+---------------+---------------+---------------------+
<span style="color: rgba(0, 128, 128, 1)"> 7</span> | <span style="color: rgba(0, 0, 255, 1)">id</span> | t_log         | t_log_type    | t_log_time          |
<span style="color: rgba(0, 128, 128, 1)"> 8</span> +----+---------------+---------------+---------------------+
<span style="color: rgba(0, 128, 128, 1)"> 9</span> |  <span style="color: rgba(128, 0, 128, 1)">2</span> | 张三          | befor_insert  | <span style="color: rgba(128, 0, 128, 1)">2021</span>-<span style="color: rgba(128, 0, 128, 1)">05</span>-<span style="color: rgba(128, 0, 128, 1)">11</span> <span style="color: rgba(128, 0, 128, 1)">20</span>:<span style="color: rgba(128, 0, 128, 1)">19</span>:<span style="color: rgba(128, 0, 128, 1)">50</span> |
<span style="color: rgba(0, 128, 128, 1)">10</span> |  <span style="color: rgba(128, 0, 128, 1)">3</span> | 张三          | befor_insert  | <span style="color: rgba(128, 0, 128, 1)">2021</span>-<span style="color: rgba(128, 0, 128, 1)">05</span>-<span style="color: rgba(128, 0, 128, 1)">11</span> <span style="color: rgba(128, 0, 128, 1)">20</span>:<span style="color: rgba(128, 0, 128, 1)">19</span>:<span style="color: rgba(128, 0, 128, 1)">50</span> |
