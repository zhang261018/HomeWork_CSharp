                if (currLoc.match(re)) { up_t = currLoc.match(re)[0] } else { if (ref.match(re)) { up_t = ref.match(re)[0] } }
                if (up_t != "") { up_t = up_t.substr(3) + ":" + ct.getTime() }
                if (uoc == "") {
                    if (getCookie(cookie_ULV) == "") {
                        uor = getHost(ref);
                        uol = getHost(currLoc)
                    }
                    setCookie(cookie_UOR, uor + "," + uol + "," + up_t, 365)
