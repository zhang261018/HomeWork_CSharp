                        setCookie(cookie_ULV, lva[0] + ":" + lva[1] + ":" + lva[2] + ":" + lva[3] + ":" + sid + ":" + lva[5], 360)
                    } else { lvr = lva[5] + ":" + lva[1] + ":" + lva[2] + ":" + lva[3] }
                } else {
                    lvn = new Date();
                    lvr = ":1:1:1";
                    setCookie(cookie_ULV, lvn.getTime() + lvr + ":" + sid + ":", 360)
                }
                return lvr
            },
            userNick: function() {
