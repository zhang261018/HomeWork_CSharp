<script>
function getScript(cfg){
	if(!cfg || !cfg.url){return;}
	var url = cfg.url,
		charset = cfg.charset || '',
		timeout = (cfg.timeout || 60000) - 0,
		timer = null,
		ontimeout = cfg.ontimeout,
		oncomplete = cfg.oncomplete,
		onsuccess = cfg.onsuccess,
		onfailure = cfg.onfailure,
		onerror = cfg.onerror;
	var s = document.createElement('script');
	if(isFinite(timeout)){
		timer = setTimeout(function(){
			s.onerror = s.onload = s.onreadystatechange = null;
			s.parentNode.removeChild(s);
			s = null;
			timer = null;
			if(typeof ontimeout === 'function'){ontimeout();}
			if(typeof onfailure === 'function'){onfailure('timeout');}
		},timeout);
	}
	s.onerror = function(){
		s.onload = s.onreadystatechange = null;
		s.parentNode.removeChild(s);
		s = null;
		timer && clearTimeout(timer);
		timer = null;
		if(typeof onerror === 'function'){onerror();}
		if(typeof onfailure === 'function'){onfailure('error');}
	};
	s.onload = s.onreadystatechange = function(){
		var r = this.readyState;
		if(!r || r === 'complete' || r === 'loaded'){
			s.onerror = s.onload = s.onreadystatechange = null;
			s.parentNode.removeChild(s);
			s = null;
			timer && clearTimeout(timer);
			timer = null;
			if(typeof oncomplete === 'function'){oncomplete();}
			if(typeof onsuccess === 'function'){onsuccess();}
		}
	};
	s.setAttribute('src',url);
	s.setAttribute('charset',charset);
	document.getElementsByTagName('head')[0].appendChild(s);
}

window.CmntLoader = function(selector){
	if(!selector){return;}
	var api = 'http://comment5.news.sina.com.cn/cmnt/count?format=json&newslist=%cids%',
		es = $(selector),
		cids = [],
		ocids = {},
		k = -1,
		i = 0,
		n = 20;
	es.each(function(){
		var cid = $(this).attr('cid');
		if(cid && !ocids[cid]){
			if(i % n === 0){
				k++;
				cids[k] = [];
			}
			cids[k].push(cid);
			ocids[cid] = 1;
			i++;
		}
	});
	if(!cids[0] || !cids[0].length){return;}
	for(i = 0, l = cids.length; i < l; i++){
		$.ajax({
			url : api.replace(/%cids%/,cids[i].join(',')),
			dataType : 'jsonp',
			cache : true,
			jsonpCallback : 'CmntLoaderPaint'
		}).done(function(s){
			if(!s || !s.result || !s.result.count){return;}
			var datas = s.result.count,
				k,n;
			for(k in datas){
				if(datas.hasOwnProperty(k)){
					n = datas[k]['show'];
					if(n > 0){n = '(' + n + ')';}
					else{n = '';}
					$(selector + '[cid="' + k + '"]').html(n);
				}
			}
		});
	}
};

window.selOpener = {
	init : function(w,btncname,opcname){
		var w = $(w);
		$(document.body).on('click',function(e){
			var t = $(e.target);
			if(t.hasClass(btncname)){
				w.toggleClass(opcname);
			}else{
				w.removeClass(opcname);
			}
		});
		return this;
	}
};

function FeedsLoader(cfg){
	this.api = cfg.api;
	this.wlist=cfg.wlist;
	this.wbtn = cfg.wbtn;
	this.wpage = cfg.wpage;
	this.wprev = cfg.wprev;
	this.selector = cfg.selector || 'li',
	this.pagen = cfg.pagen || 5;
	this.step = cfg.step || 3;
	this.size = cfg.size || 8;
	this.cname = cfg.cname;
	this.self = cfg.self;
	this.joiner = cfg.joiner || function(){};
	this.callback = cfg.callback || function(){};
    this.onblank = cfg.onblank || function(){},
	this.total = 0;
	this.nIndex = 0;
	this.isInit=false;
	this.init();
};
FeedsLoader.prototype = {
	init : function(){
		this.initEvent();
		this.go(1);
	},
	initEvent : function(){
		var that = this,
			wbtn = that.wbtn,
			timer;
		$(window).on('scroll',function(){
			timer && clearTimeout(timer);
			timer = setTimeout(function(){
				if(wbtn.is(':visible') && wbtn.offset().top < $(window).scrollTop() + $(window).height() - 150){
					that.showMore(false);
				}
			},20);
		});
		that.wpage.delegate('a','click',function(e){
			var t = $(this);
			if(t.hasClass('disabled')){return;}
			if(t.hasClass('llt')){
				e.preventDefault();
				that.llt();
				return;
			}
			if(t.hasClass('rrt')){
				e.preventDefault();
				that.rrt();
				return;
			}
			if(t.hasClass('lt')){
				e.preventDefault();
				that.lt();
				return;
			}
			if(t.hasClass('rt')){
				e.preventDefault();
				that.rt();
				return;
			}
			e.preventDefault();
			that.go(t.attr('page'));
		});
	},
	showMore : function(b){
		var that = this,
			nIndex = that.nIndex,
			selector = that.selector,
			wlist=that.wlist,
			wprev = that.wprev,
			wpage = that.wpage,
			wbtn = that.wbtn,
			lists=$(selector,wlist),
			hlists=$(selector+':hidden',wlist),
			ll = lists.length,
			hll = hlists.length,
			size = that.size,
			l = Math.min(hll,size),
			i;
		if(nIndex === 1 && wprev.length > 0 && $(selector,wprev).length){
			wprev.show();
			if(b){
				if(hll > 0){
					wbtn.show();
					wpage.hide();
				}else{
					wbtn.hide();
					wpage.show();
				}
				return;
			}else{
				if(hll > size){
					wbtn.show();
					wpage.hide();
				}else{
					wbtn.hide();
					wpage.show();
				}
			}
		}else{
			wprev.hide();
			if(hll > size){
				wbtn.show();
				wpage.hide();
			}else{
				wbtn.hide();
				wpage.show();
			}
		}
		for(i = 0; i < l; i++){
			$(hlists[i]).show();
		}
	},
	paintPage : function(){
		var that = this,
			wpage = that.wpage,
			cname = that.cname,
			total = that.total,
			size = that.size,
			step = that.step,
			pagen = that.pagen,
			mpage = Math.ceil(total / size / step),
			nIndex = that.nIndex,
			l,s,e,
			h = [];
		wpage.html('');
		if(mpage <= 1){return;}
		s = 1 + pagen * Math.floor((nIndex - 1) / pagen);
		e = Math.min(mpage,s + pagen - 1);
		if(s > pagen){h.push('<a href="javascript:;" class="llt">上' + pagen + '页</a>');}
		if(nIndex === 1){h.push('<a href="javascript:;" class="disabled">上一页</a>');}
		else{h.push('<a href="javascript:;" class="lt">上一页</a>');}
		for(; s <= e; s++){
			h.push('<a href="javascript:;" page="' + s + '" class="' + (s === nIndex ? cname : '') + '">' + s + '</a>');
		}
		if(nIndex >= mpage){h.push('<a href="javascript:;" class="disabled">下一页</a>');}
		else{h.push('<a href="javascript:;" class="rt">下一页</a>');}
		if(e < mpage){h.push('<a href="javascript:;" class="rrt">下' + pagen + '页</a>');}
		wpage.html(h.join(''));
	},
	llt : function(){
		var that = this,
			pagen = that.pagen;
		that.go(1 + pagen * (Math.floor((that.nIndex - 1) / pagen) - 1));
	},
	rrt : function(){
		var that = this,
			pagen = that.pagen;
		that.go(1 + pagen * (Math.floor((that.nIndex - 1) / pagen) + 1));
	},
	lt : function(){
		var that = this;
		that.go(that.nIndex - 1);
	},
	rt : function(){
		var that = this;
		that.go(that.nIndex + 1);
	},
	go : function(page){
		var that = this,
			mpage = Math.ceil(that.total / that.step / that.size),
			api = that.api,
			size = that.size,
			step = that.step,
			num;
		if (!arguments.callee.count){
            arguments.callee.count=1;
		} else{
			that.isInit=true;
		}
		page = parseInt(page);
		if(isNaN(page)){return;}
		if(page === that.nIndex){return;}
		if(page <= 0){that.nIndex = 1;}
		else if(page > mpage){that.nIndex = Math.max(1,mpage);}
		else{that.nIndex = page;}
		num = size * step;
		api = api.replace(/%page%/,page).replace(/%num%/,num);
		$.ajax({
			url : api,
			cache : true,
			dataType : 'jsonp',
			jsonpCallback : that.self + '.paint'
		});
	},
	paint : function(s){
		if(!s || !s.result || !s.result.list || !s.result.list.length){
			if(this.nIndex > 1){
				this.go(this.nIndex - 1);
				return;
			} else if(!this.wprev.length || !$(this.selector,this.wprev).length){
                this.onblank(s);
                return;
            }
		}
		var that = this,
			nIndex = that.nIndex,
			wlist=that.wlist,
			odatas = s.result.list,
			pageNo = s.result.page,
			datas = [],
			l,
			h = [],
			i;
		if(nIndex === 1 && that.wprev.length > 0 && $(that.selector,that.wprev).length){
			for(i = 0, l = odatas.length; i < l; i++){
				if($('[bid="' + odatas[i]['articleurl'] + '"]').length > 0){continue;}
				datas.push(odatas[i]);
			}
			l = (that.step - 1) * that.size;
		}else{
			datas = odatas;
			l = that.step * that.size;
		}
		that.total = s.result.count - 0;
		i=(!that.isInit&&pageNo==1)?8:0;
		for(i ; i < l; i++){
			h.push(that.joiner(datas[i]));
		}
		(!that.isInit&&pageNo==1)?wlist.append(h.join('')):wlist.html(h.join(''));
		$(that.selector,wlist).hide();
		that.showMore(true);
		that.paintPage();
		that.callback();
	}
};

selOpener.init('#header_more','header_more_btn','header_more_open').init('#header_msg','header_msg_btn','header_msg_open').init('#header_info','header_info_btn','header_info_open');

new DivSelect('searcher_sel');


(function(){
	var w1 = $('#header_user_unlogin'),
		w2 = $('#header_user_logined'),
		w3 = $('#header_msg dd'),
		wimg = $('img',w2),
		wmsg = $('#header_msg dt',w2),
		wdm = $('i[msgtype="dm"]',w2),
		wcmt = $('i[msgtype="cmt"]',w2),
		wat = $('i[msgtype="at"]',w2),
		interval = null,
		api = 'https://rm.api.weibo.com/2/remind/unread_count.json',
		initUserInfo = function(uid){
			wimg.attr('src','http://tp2.sinaimg.cn/' + uid + '/180/5672194171/1');
			getUnread(uid);
			interval && clearInterval(interval);
			interval = setInterval(function(){
				getUnread(uid);
			},60 * 1000);
		},
		getUnread = function(uid){
			$.ajax({
				url : api,
				data : {
					source : 2751753920,
					uid : uid
				},
				dataType : 'jsonp',
				cache : true
			}).done(function(s){
				if(!s || !s.data){return;}
				var datas = s.data,
					dm = datas['dm'] || '',
					cmt = datas['cmt'] || '',
					at = datas['mention_status'] || '';
				if(dm){wdm.html(dm);}
				if(cmt){wcmt.html(cmt);}
				if(at){wat.html(at);}
				if(dm || cmt || at){wmsg.addClass('header_msg_new');}
			});
		};
	w3.on('click',function(){
		wdm.html('');
		wcmt.html('');
		wat.html('');
		wmsg.removeClass('header_msg_new');
	});
	window.USERSET = {
		refresh : function(s){
			if(!s || !s.uid){
				w1.show();
				w2.hide();
				interval && clearInterval(interval);
			}else{
				w1.hide();
				w2.show();
				initUserInfo(s.uid);
			}
		}
	};
})();

(function(){
	var cont = $('#feeds'),
		tag = PAGECFG.tag.name;
	if(!window['feedsLoader']){
		window['feedsLoader'] = new FeedsLoader({
			api : 'http://api.search.sina.com.cn/?q=' + encodeURIComponent(tag) + '&c=news&page=%page%&num=%num%&oe=utf-8&range=keyword' + (PAGECFG.app == 'ent' ? '&col=1_8' : ''),
			wlist : $('.feeds_list',cont),
			wbtn : $('.feeds_more',cont),
			wpage : $('.feeds_page',cont),
			wprev : $('.feeds_prev',cont),
			pagen : 5,
			step : 3,
			size : 6,
			cname : 'selected',
			self : 'feedsLoader',
			joiner : function(d){
				if(!d){return '';}
				var tli = '<li bid="%url%"><h3 class="feeds_li_title"><a href="%url%" target="_blank" title="%title%">%stitle%</a></h3><div class="feeds_li_cont clearfix">%img%<div class="feeds_li_t clearsub"><p><a href="%url%" target="_blank">%content%</a><a href="%url%" target="_blank" class="feeds_li_detail">[详细]</a></p></div></div><div class="feeds_li_meta clearfix"><span class="fl"><a href="%url%" target="_blank">%ptime%</a>%tags%</span><a href="%url%" target="_blank">评论<i class="feeds_li_cmnt" cid="%cid%">(0)</i></a> | <a href="javascript:;" id="bdshare" class="bdshare_t bds_tools get-codes-bdshare" data="{text:%title%,url:%url%,pic:%imgurl%}"><i class="bds_more">分享</i></a></div></li>',
                ttags = '<a href="http://tags.'+PAGECFG.app+'.sina.com.cn/%tagen%" target="_blank" class="feeds_li_tags %c%">%tag%</a>',
                    timg = '<div class="feeds_li_p %isvideo% fl"><a href="%url%" target="_blank"><img src="%imgurl%" alt="%title%" title="%title%" /></a><a href="%url%" target="_blank" class="feeds_li_video"></a></div>',
					url = d['url'] || '',
                    isvideo = d['type'] == 2 ? 'feeds_li_p_video' : '',
					title = (d['origin_title'] || d['title'] || '').replace(/&nbsp;/g,' '),
					stitle = (d['title'] || d['origin_title'] || d['short_title']).replace(/&nbsp;/g,' '),
					content = d['intro'] || '',
					ptime = d['datetime'] || '',
					ccid = d['cid'] || '',
					cid = /\:\d$/.test(ccid) ? ccid : ccid ? ccid + ':0' : '',
					imgurl = d['imgurl'],
					img = imgurl ? timg : '',
					kl = d['kl'] || '',
					tags = (function(kl,n){
						if(!kl){return '';}
						var ks = kl.split(/[,，\s]/),
							l = ks.length,
							ret = [],
							i,d;
						for(i = 0; i < l; i++){
							d = ks[i];
							ret.push(ttags.replace(/%tagen%/g,encodeURIComponent(d)).replace(/%tag%/g,d).replace(/%c%/g,d === tag ? 'cred' : ''));
						}
						if(!ret.length){return '';}
						if(typeof n === 'number'){ret = ret.slice(0,n);}
                        return '&emsp;标签：' + ret.join('&emsp;');
					})(kl,5),
					ret = tli.replace(/%img%/g,img).replace(/%url%/g,url).replace(/%title%/g,title).replace(/%stitle%/g,stitle).replace(/%content%/g,content).replace(/%ptime%/g,ptime).replace(/%cid%/g,cid).replace(/%tags%/g,tags).replace(/%imgurl%/g,imgurl).replace(/%isvideo%/g,isvideo);
				return ret;
			},
			callback : function(){
				$('html,body').animate({
					scrollTop : 0
				});
				if(window.CmntLoader){CmntLoader('#feeds i.feeds_li_cmnt');}
				if(window.bdShare && bdShare.fn && bdShare.fn.init){bdShare.fn.init();}
            },
            onblank : function(s){
                if(s && s.result && s.result.ALLOWSEARCH === "1"){cont.html('<h3 style="font-weight:400;margin-top:20px;font-size:16px;">标签结果可能不符合相关法律法规和政策，未予显示</h3>');}
                else{cont.html('<h3 style="font-weight:400;margin-top:20px;font-size:16px;">最近没有关于“' + tag + '”的新闻，看看其他标签的内容吧</h3>');}
            }

		});
	}
})();

(function(){
	var wid = 'main_guess_slide',
		w = $('#' + wid),
		wnext = $('#main_guess_change'),
		page = 5,
		num = 7,
		fields = ['title','url','stitle','ctime','thumb'],
		api = 'http://cre.mix.sina.com.cn/api/v3/get?cateid=sina_all&length=%length%&fields=%fields%&callback=guessCallback',
		tli = '<li class="clearfix">%pic%<div class="list01_t clearsub"><p><a href="%url%" target="_blank">%stitle%</a></p><p class="list01_time">%ptime%</p></div></li>',
		tpic = '<div class="list01_p fl"><a href="%url%" target="_blank"><img src="%imgurl%" alt="%title%" title="%title%" /></a></div>',
		sp = null,
		gdata = null,
		fixNum = function(n){
			n = parseInt(n);
			return n < 10 ? '0' + n : '' + n;
		},
		handdleFailure = function(){
			paint();
		},
		paint = function(s){
			var datas = s || window.TAGGUESSDATA || [],
				h = [],
				l = datas.length,
				x = 0,
                url,
				y = 0,
				i,d;
			for(i = 0; i < l; i++){
				d = datas[i];
				if(i % num === 0){
					h.push('<div class="main_guess_slide_items fl">');
					h.push('<ul class="list01">');
					x++;
					y = 0;
				}
                y++;
                url = d['url'] || '';
                url = (url.indexOf('?') > -1 ? url+'&' : url+'?') + 'cre=tagspc&mod=g&r=' + (d['r'] || 'user') + '&pos=' + x + '_' + y;
				h.push(tli.replace(/%pic%/,d['thumb'] ? tpic.replace(/%imgurl%/,d['thumb']) : '').replace(/%url%/g,url).replace(/%title%/g,d['title'] || '').replace(/%stitle%/g,d['stitle'] || d['title'].slice(0,20)).replace(/%ptime%/,(function(q){
					if(!q){return '';}
					q = new Date(q * 1000);
					return (q.getMonth() + 1) + '月' + q.getDate() + '日 ' + fixNum(q.getHours()) + ':' + fixNum(q.getMinutes());
				})(d['ctime'])));
				if(i % num === num - 1 || i === l - 1){
					h.push('</div>');
					h.push('</ul>');
				}
			}
			w.html(h.join(''));
			sp = new ScrollPic();
			sp.scrollContId = wid;
			sp.pageWidth = 300;
			sp.frameWidth = 300;
			sp.speed = 10;
			sp.autoPlay = false;
			sp.initialize();
		};
	wnext.on('click',function(){
		if(sp){sp.next();}
	});
	window.guessCallback = function(s){
		var datas;
		if(!s){
			handdleFailure();
			return;
		}
		datas = (function(d,t){
			var ti,tl,td;
			d = d || [];
			t = t || [];
			for(ti = 0, tl = t.length; ti < tl; ti++){
				td = t[ti];
				td['r'] = 'hot';
				d.push(td);
			}
			return d;
		})(s.data,s.top);
		if(!datas.length){
			handdleFailure();
			return;
		}
		gdata = datas;
	};
	window.GUESSSLIDE = {
		refresh : function(){
			getScript({
				url : api.replace(/%length%/,page * num).replace(/%fields%/,fields.join(',')) + '&_=' + new Date().getTime(),
				timeout : 10e3,
				ontimeout : function(){
					handdleFailure();
				},
				onerror : function(){
					handdleFailure();
				},
				oncomplete : function(){
					paint(gdata);
				}
			})
		}
	};
})();


(function(){
        var w = $('#main_guess'),
        w3 = $('.main_l'),
        w4 = $('.main_r'),
        interval = null,
        timer = null,
        resizePos = function(){
            var hpl = w3.height(),
                hpr = w4.height(),
                tpl = w3.offset().top,
                tpr = w4.offset().top,
                st = $('document,body').scrollTop(),
                hh = w.height();
            if(st > tpr + hpr - 10){
                w.addClass('main_guess_fixed');
                w.css({
                    top : Math.min(0,hpl + tpl - st - hh) + 'px'
                });
            }else{
                w.removeClass('main_guess_fixed');
                w.css({
                    top : 0
                });
            }
        };
    interval = setInterval(resizePos,20);
    $(window).on('load',function(){
        interval && clearInterval(interval);
        interval = null;
        $(window).on('scroll',function(){
            timer && clearTimeout(timer);
            timer = setTimeout(resizePos,20);
        });
    })
})();
</script>
<script>
(function(){
	var logined = !sinaSSOController.getSinaCookie();
	var checkLogin = function(){
		var s = sinaSSOController.getSinaCookie(),
			islogin;
		if(s){islogin = true;}
		else{islogin = false;}
		if(islogin === logined){return;}
		logined = islogin;
		if(window.USERSET && USERSET.refresh){USERSET.refresh(s);}
		if(window.GUESSSLIDE && GUESSSLIDE.refresh){GUESSSLIDE.refresh();}
	};
	setInterval(checkLogin,200);
})();
</script>


