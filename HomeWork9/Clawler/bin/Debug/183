<script type="text/javascript" src="http://i.sso.sina.com.cn/js/outlogin_layer.js" charset="utf-8"></script>
<script type="text/javascript" src="http://i.sso.sina.com.cn/js/user_panel_new_version.js" charset="utf-8"></script>
<script type="text/javascript" src="http://news.sina.com.cn/js/87/20140926/top.js"></script>
<!-- 登录初始化 -->
<script type="text/javascript">
(function(Topbar){

	var needReload = false;


    // 登录
    Topbar.user.init(document.getElementById('SI_User'),{
        // entry 默认为 account，各频道不一样，可向通行证@龚翼 申请
        entry:'account',
        // 登录成功
        login_success:function(){
        	if(needReload){
	            location.href = location.href;
        	}
        },
        // 退出成功
        logout_success:function(){
            location.href = location.href;
        }
    });
    // 提示
    var Tip = (function(){
    	var wrap = null;
    	var calculator = null;
    	var timer = null;
    	return {
    		show:function(ele,html,time){
    			clearTimeout(timer);
    			var self = this;
    			if(!wrap){
    				wrap = $('<div class="subscribe-tip" style="display:none;"></div>').appendTo(document.body);
    			}
    			wrap.hide();
    			var offset = ele.offset();
    			wrap.html(html);
    			wrap.show().css({top:0, left:0});
    			wrap.css({
    			  top: (offset.top + wrap.outerHeight()) + 'px',
    			  left: (offset.left - wrap.outerWidth()/2) + 'px'
    			});
    			if(time){
    				timer = setTimeout(function(){
    					self.hide();
    				},time);
    			}
    		},
    		hide:function(){
    			wrap.hide();
    		}
    	};
    })();
	var timeoutHandle = (function(){
	    var events = [];
	    var handle = {
	        success:function(id){
	            var eve = events[id];
	            if(!eve){
	                return;
	            }
	            eve.isSuccess = true;
	            clearTimeout(eve.timer);

	        },
	        timeout:function(id,fn){
	            var eve = events[id];
	            if(!eve){
	                return;
	            }
	            eve.timer = setTimeout(function(){
	                if(eve.isSuccess){
	                    return;
	                }
	                if(typeof fn == 'function'){
	                    fn.call(this);
	                }
	            },eve.time);
	        }
	    };
	    return function(id,fn,time){
	        if(events[id]){
	            throw new Error(id+'已经被占用');
	            return;
	        }
	        events[id] = {};
	        events[id].time = time||5e3;
	        events[id].isSuccess = false;
	        if(typeof fn == 'function'){
	            fn.call(this,handle);
	        }
	    }
	})();
	var ijax = function(url,data,cb){
		var body = $('body');
		var suffix = Math.abs((new Date()).getTime()) + '_' + Math.round(Math.random() * 1e8);
		var id = "ijax_iframe_"+suffix;
		var ifm = $('<iframe frameborder="0" name="'+id+'" id="'+id+'"></iframe>');
		var inputHtml = (function(data){
			var html = [];
			for(var name in data){
				var val = data[name];
				html.push('<input type="hidden" name="'+name+'" value="'+val+'" />');
			}
			return html.join('');
		})(data);
		window['ijax_'+suffix] = function(msg){
			if(typeof cb === 'function'){
				cb(msg);
			}
			setTimeout(function(){
				ifm.remove();
				form.remove();
			},1e3);
		};
		var form = $('<form action="'+url+'?callback=ijax_'+suffix+'" method="post" target="'+id+'">'+inputHtml+'</form>');
		body.append(ifm);
		body.append(form);
		form.submit();

	};
	var subscribe = (function(){
		var adding = false;

		var add = function(allData,word,cb){
			var data = {
				project_id:allData.projectId,
				// data:'[{c:"'+ word +'",w:0}]',
				'data[0][c]':word,
				'data[0][w]':0,
				token:'',
				dt:'',
				from:'pc'
			};
			ijax('http://user.mix.sina.com.cn/subscribe/api/subscribe/update',data,function(msg){
				cb(msg);
			});
		};
		var remove = function(allData,word,cb){
			var data = {
				project_id:allData.projectId,
				'data[0][c]':word,
				token:'',
				dt:'',
				from:'pc'
			};
			ijax('http://user.mix.sina.com.cn/subscribe/api/subscribe/del',data,function(msg){
				cb(msg);
			});
		};
		var changeBtn = function(wrap,btnType){
			if(btnType==='a'){
				if (wrap.hasClass('btn-a-done')) {
					wrap.html('<i></i>订阅');
					wrap.removeClass('btn-a-done');
				} else {
					wrap.html('已订阅');
					wrap.addClass('btn-a-done');
				}
			}else{
				var type = 'add';
				if (wrap.hasClass('btn-b-done')) {
					type = 'remove';
					wrap.removeClass('btn-b-done btn-b-done-hover');
				} else {
					wrap.addClass('btn-b-done btn-b-done-hover');
				}
				toggleBtn(wrap,type);
			}

		};
		var toggleBtn = function(wrap,type){
			var isHot = wrap.parents('[node-type=tagList]').length>0;
			if(isHot){
				var oldWrap = dom.myTagList.find('[tag='+wrap.text()+']');
				if(type==='add'){
					// add时，如果是在hot中，复制一份并添加到my中
					if(oldWrap.length===0){
						var newWrap = wrap.clone();
						newWrap.appendTo(dom.myTagList);
						newWrap.removeClass('btn-b-hover btn-b-done-hover');
					}
				}else{
					oldWrap.remove();
				}

				// remove时，在my中找到相同的标签删掉
			}else{
				// 在my中直接删掉
				if(type==='remove'){
					wrap.remove();
				}
			}
			dom.myTagList.parent().show();

		};
		return function(item,allData,btnType){
			// 判断是否登录
			var UserPanel = window.SINA_USER_PANEL;
			LoginLayer = UserPanel.getOutLogin();
			if(!LoginLayer.isLogin()){
				try{
					UserPanel.setOutLoginMiddle(true);
					UserPanel.getOutLogin().show();
					return;
				}catch(e){

				}
				needReload = true;
			}
			var wrap = item.parent();
			var word = wrap.find('.t').text();
			if(btnType === 'a'){
				wrap = item;
				word = PAGECFG.tag.name;
			}
			if(adding){
				Tip.show(item,'您操作太快了，请休息一会！',2e3);
				return;
			}

			// 订阅类型 add为添加 remove为删除
			var type = wrap.hasClass('btn-b-done')?'remove':'add';
			if(btnType === 'a'){
				type = wrap.hasClass('btn-a-done')?'remove':'add';
			}
			adding = true;
			var TIMEOUT_NAME = 'TD_'+ (new Date()).getTime();
			timeoutHandle(TIMEOUT_NAME,function(handle){
			    handle.timeout(TIMEOUT_NAME,function(){
			        adding = false;
			    });
			    if(type==='add'){
			    	add(allData, word,function(msg){
			    		if(msg&&msg.result&&msg.result.status.code===0){
				    		changeBtn(wrap,btnType);
			    		}else{
			    			Tip.show(item,'服务器罢工，请稍后再试！',2e3);
			    		}
			    		adding = false;
			    		// 成功通知
			    		handle.success(TIMEOUT_NAME);

			    	});
			    }else{
			    	remove(allData, word,function(msg){
			    		if(msg&&msg.result&&msg.result.status.code===0){
				    		changeBtn(wrap,btnType);
			    		}else{
			    			Tip.show(item,'服务器罢工，请稍后再试！',2e3);
			    		}
			    		adding = false;
			    		// 成功通知
			    		handle.success(TIMEOUT_NAME);

			    	});
			    }

			},2e3);

		};
	})();
	var render = function(dom,allData){
		var renderCont = function(){
			var contClz = '';
			var tag = PAGECFG.tag.name;
			var kuEnt = allData.kuEnt;
			// 渲染图片和内容
			var data = {};
			var html = [];
			var pic = 'http://www.sinaimg.cn/dy/deco/2014/1215/d.png';
			var noPic = true;
			if(kuEnt&&kuEnt.status&&kuEnt.status.code===0){
				data = kuEnt.data;
			}
			if($.isArray(data)&&data.length>0){
				data = data[0];
				if(data.pic){
					noPic = false;
					pic = data.pic;
				}

			}else{
				data.desc = '';
			}
			if(data.desc===''){
				contClz += ' no-txt';
			}
			html.push('<div class="img fl">');
			if(noPic){
				contClz +=' no-img';
			}else{
				html.push('<img src="http://s.img.mix.sina.com.cn/auto/resize?img='+encodeURIComponent(pic)+'&size=136_170" height="170" width="136" alt="'+tag+'">');
			}
			html.push('</div>');
			html.push('<div class="txt">');
			html.push('<div class="hd clearfix">');
			html.push('<span class="name fl"><i></i><em>'+tag+'</em></span>');
			if(allData.tag.status === 0){
				html.push('<a class="btn-a fl" href="javascript:;" title="订阅"><i></i>订阅</a>');
			}else{
				html.push('<a class="btn-a btn-a-done" href="javascript:;" title="已订阅"><i></i>已订阅</a>');
			}
			html.push('</div>');
			html.push('<div class="bd">'+ strSplit(data.desc,270)+'&nbsp;');
			if(data.desc){
				html.push('<a href="'+data.url+'" target="_blank">详情&gt;&gt;</a>');
			}
			html.push('</div>');
			html.push('</div>');
			html = html.join('');
			dom.cont.addClass(contClz);
			dom.cont.html(html).show();
		};
		var renderTag = function(item){
			var clz = 'btn-b';
			if(item.status===1){
				clz += ' btn-b-done';
			}
			item.url = item.url ||('http://'+location.host+'/'+item.name);
			return '<span class="'+clz+'" href="javascript:;" tag="'+item.name+'"><a href="'+item.url+'" class="t">'+item.name+'</a><a href="javascript:;" class="i"></a></span>';
		};
		var renderTagList = function(listDom,listData){
			if(typeof listData === 'undefined'|| ($.isArray(listData)&&listData.length===0)){
				return;
			}
			var html = [];
			for (var i = 0, len = listData.length; i < len; i++) {
				var item = listData[i];
				html.push(renderTag(item));
			}
			listDom.html(html.join(''));
			listDom.parent().show();
		};
		renderCont();
		renderTagList(dom.tagList,allData.tagList);
		renderTagList(dom.myTagList,allData.myTagList);
	};
	var strSplit = (function(){
        var byteLen = function(str){
            if(typeof str == "undefined"){
                return 0;
            }
            var aMatch = str.match(/[^\x00-\x80]/g);
            return (str.length + (!aMatch ? 0 : aMatch.length));
        };
        return function(str,len){
            var s = str.replace(/\*/g, " ").replace(/[^\x00-\xff]/g, "**");
            str = str.slice(0, s.slice(0, len).replace(/\*\*/g, " ").replace(/\*/g, "").length);
            if(byteLen(str) > len) str = str.slice(0,str.length -1);
            return str;
        };
    })();
	var getNodes = function(wrap) {
        var attr = 'node-type';
        var wrap = $('#' + wrap);
        var nodes = $("[" + attr + "]", wrap);
        var nodesObj = {};
        nodesObj.wrap = wrap;
        nodes.each(function(i) {
            var item = $(this);
            nodesObj[item.attr(attr)] = item;
        });
        return nodesObj;
    };
    var bind = function(dom){
    	var over = function(item){
    		var hoverClz = 'btn-b-hover';
    		if(item.hasClass('btn-b-done')){
    			hoverClz = hoverClz+' btn-b-done-hover';
    		}
    		item.addClass(hoverClz);
    	};
    	var out = function(item){
    		var hoverClz = 'btn-b-hover';
    		if(item.hasClass('btn-b-done')){
    			hoverClz = hoverClz+' btn-b-done-hover';
    		}
    		item.removeClass(hoverClz);
    	};
    	dom.tagList.on('mouseover','.btn-b',function(){
    		over($(this));
    	});
    	dom.tagList.on('mouseout','.btn-b',function(){
    		out($(this));
    	});
    	// 已订阅标签
    	dom.myTagList.on('mouseover','.btn-b',function(){
    		over($(this));
    	});
    	dom.myTagList.on('mouseout','.btn-b',function(){
    		out($(this));
    	});
    	// 订阅
    	dom.tagList.on('click','.i',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'b');
    	});
    	dom.myTagList.on('click','.i',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'b');
    	});
    	dom.cont.on('click','.btn-a',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'a');
    	});
    };
    var dom = {
    	cont:$('#SI_Cont'),
    	tagList:$('#SI_Tags'),
    	myTagList:$('#SI_MyTagList')
    };

	render(dom,PAGECFG);
    bind(dom);
})(window.__SinaTopBar__);
</script>
